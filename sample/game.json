{
	"width": 320,
	"height": 320,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"HoverableRect": {
			"type": "script",
			"path": "script/HoverableRect.js",
			"global": true
		},
		"HoverableLabel": {
			"type": "script",
			"path": "script/HoverableLabel.js",
			"global": true
		},
		"aco": {
			"type": "image",
			"width": 128,
			"height": 288,
			"path": "image/aco.png"
		},
		"hover_plugin": {
			"type": "script",
			"path": "node_modules/@akashic-extension/akashic-hover-plugin/lib/HoverPlugin.js",
			"global": true
		}
	},
	"operationPlugins": [
		{
			"code": 1,
			"script": "./node_modules/@akashic-extension/akashic-hover-plugin/lib/HoverPlugin.js",
			"option": {
				"showTooltip": true
			}
		}
	],
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/Converter.js",
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/index.js"
	],
	"moduleMainScripts": {
		"@akashic-extension/akashic-hover-plugin": "node_modules/@akashic-extension/akashic-hover-plugin/lib/index.js"
	},
	"environment": {
		"sandbox-runtime": "2"
	}
}